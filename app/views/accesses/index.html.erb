<div class="page-header">
    <h1>CDN Accesses</h1>
</div>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>#</th>
            <th>&nbsp;</th>
        </tr>
    </thead>
    <tbody>
    <% if @accesses.count > 0 %>
        <% @accesses.each do |a| %>
        <tr>
            <td><%= link_to a.identifier, access_path(a) %></td>
            <td><%= link_to 'Edit', edit_access_path(a) %></td>
        </tr>
        <% end %>
    <% else %>
        <tr>
            <td colspan="2">No CDN acess</td>
        </tr>
    <% end %>
    </tbody>
</table>

<%= link_to 'New', new_access_path() %>