<div class="page-header">
    <h1>CDN Access</h1>
</div>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>#</th>
        </tr>
    </thead>
    <tbody>
    <% if @access.count > 0 %>
        <% @access.each do |a| %>
        <tr>
            <td><%= a.identifier %></td>
        </tr>
        <% end %>
    <% else %>
        <tr>
            <td colspan="13">No cdn acess</td>
        </tr>
    <% end %>
    </tbody>
</table>
