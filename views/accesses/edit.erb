<div class="page-header">
    <h1>New CDN Access</h1>
</div>

<form action="/accesses/<%= @access.id %>" method="post">
    <input type="hidden" name="_method" value="PUT" />
  <label for="post_identifier">Identifier:</label><br />
  <input id="post_identifier" name="access[identifier]" type="text" value="<%= @access.identifier %>" />
  <br />

  <input type="submit" value="Edit Access" />
</form>