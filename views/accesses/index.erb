<div class="page-header">
    <h1>CDN Access</h1>
</div>

<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>#</th>
            <th>&nbsp;</th>
        </tr>
    </thead>
    <tbody>
    <% if @accesses.count > 0 %>
        <% @accesses.each do |a| %>
        <tr>
            <td><a href="<%= url_for "/accesses/#{a.id}" %>"><%= a.identifier %></a></td>
            <td><a href="<%= url_for "/accesses/edit/#{a.id}" %>">Edit.</a></td>
        </tr>
        <% end %>
    <% else %>
        <tr>
            <td colspan="13">No cdn acess</td>
        </tr>
    <% end %>
    </tbody>
</table>
